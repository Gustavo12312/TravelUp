{
  "expo": {
    "name": "TravelUp",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/airplane.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,   
    "ios": {
      "supportsTablet": true   
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {          
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ADD8E6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
